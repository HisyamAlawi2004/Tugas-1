    <script>
        var Nilai = prompt("Berapa Nilaimu?");
        
        

        
        if(90 < Nilai && Nilai < 100 || Nilai == 90 || Nilai == 100 ){
            alert("Kamu Sangat Baik");
        }else if(80 < Nilai && Nilai < 90 || Nilai == 80 || Nilai == 89){
            alert("Baik");
        }else if(75 < Nilai && Nilai < 80 || Nilai == 75 || Nilai == 79){
            alert("Masih Diatas KKM");
        }else if(Nilai < 75){
        alert(":'( Dibawah kkm");
        }else{
            alert('Hmmmm......Salah Masukin nilai ya');
        }
    </script>
    
